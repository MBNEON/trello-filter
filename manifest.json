{
	"name": "'Cards View' Filter for Trello",
	"version": "0.35",
	"manifest_version": 2,
	"description": "Small extension that adds filtering by list in Trello's 'Cards' view.",
	"content_scripts": [{
	 "js": ["js/jquery-2.0.2.min.js", "filter.js"],
	 "css": ["filter.css"],
	 "matches": ["*://trello.com/*"]
	}],
	"icons": { "48": "trello-card-filter-48.png",
	"128": "trello-card-filter-128.png" }
}